{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "Meta": {
      "type": "object",
      "properties": {
        "DataModel": {
          "type": "string"
        },
        "EventType": {
          "type": "string"
        },
        "EventDateTime": {
          "type": "string"
        },
        "Test": {
          "type": "boolean"
        },
        "Source": {
          "type": "object",
          "properties": {
            "ID": {
              "type": "string"
            },
            "Name": {
              "type": "string"
            }
          }
        },
        "Destinations": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "ID": {
                "type": "string"
              },
              "Name": {
                "type": "string"
              }
            }
          }
        },
        "Message": {
          "type": "object",
          "properties": {
            "ID": {
              "type": "number"
            }
          }
        },
        "Transmission": {
          "type": "object",
          "properties": {
            "ID": {
              "type": "number"
            }
          }
        },
        "FacilityCode": {
          "type": "string"
        }
      },
      "required": [
        "DataModel",
        "EventType"
      ]
    },
    "Patient": {
      "type": "object",
      "properties": {
        "Identifiers": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "ID": {
                "type": "string"
              },
              "IDType": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    "StartDateTime": {
      "type": "string"
    },
    "EndDateTime": {
      "type": "string"
    },
    "Visit": {
      "type": "object",
      "properties": {
        "Reasons": {
          "type": "array",
          "items": {}
        },
        "AttendingProviders": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "ID": {
                "type": "string"
              },
              "IDType": {
                "type": "string"
              },
              "FirstName": {
                "type": "string"
              },
              "LastName": {
                "type": "string"
              },
              "Credentials": {
                "type": "array",
                "items": {}
              },
              "Address": {
                "type": "object",
                "properties": {
                  "StreetAddress": {
                    "type": "string"
                  },
                  "City": {
                    "type": "string"
                  },
                  "State": {
                    "type": "string"
                  },
                  "ZIP": {
                    "type": "string"
                  },
                  "County": {
                    "type": "string"
                  },
                  "Country": {
                    "type": "string"
                  }
                }
              },
              "Location": {
                "type": "object",
                "properties": {
                  "Type": {
                    "type": "string"
                  },
                  "Facility": {
                    "type": "string"
                  },
                  "Department": {
                    "type": "string"
                  },
                  "Room": {
                    "type": "string"
                  }
                }
              },
              "PhoneNumber": {
                "type": "object",
                "properties": {
                  "Office": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "Locations": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "Type": {
                "type": "string"
              },
              "Facility": {
                "type": "string"
              },
              "Department": {
                "type": "string"
              },
              "Room": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  },
  "required": [
    "Meta",
    "StartDateTime"
  ]
}